<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title></title>
	</head>
	<body>
		<script>
			var arr = [3,1,4,2,9,8,0]
			for(var i = 0;i<arr.length; i++){
				for(var j=0;j<arr.length-1;j++){
					if(arr[j]>arr[j+1]){
						var temp = arr[j+1];
						arr[j+1] = arr[j]
						arr[j] = temp;
						
					}
				}
			}
			console.log(arr);
		</script>
	</body>
</html>
